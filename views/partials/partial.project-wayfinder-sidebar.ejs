<div class="sidebar_box admin_box sb_container">
    <div class="sidebar_box_title">Wayfinder</div>
    <div class="sidebar_box_content">
	    <div class="wayfinder sidebar_box_text">
            <ul class="check_list">
<% for (i in project.tasks) { %>
                <li class="
                    <% if (project.tasks[i].completed()) { %> completed<% } %>">
                  <button class="wayfinder_task" id="wayfinder_add_media"><%= project.tasks[i].label %></button></li>
<% } %>
            </ul>
        </div>
        <div class="clear"></div>
    </div>
</div>

